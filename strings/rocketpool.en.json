{
  "reth_burn_event": {
    "title": ":fire: rETH Burn",
    "description": "Burned %{amount} rETH for %{ethAmount} ETH!"
  },
  "minipool_deposit_event": {
    "title": ":rocket: Minipool Staked",
    "description": "Minipool %{minipool_fancy} has deposited to the Beacon Chain!"
  },
  "minipool_destroy_event": {
    "title": ":firecracker: Minipool Destroyed",
    "description": "Minipool %{minipool_fancy} has been destroyed!"
  },
  "pool_deposit_event": {
    "title": ":rocket: Pool Deposit",
    "description": "%{amount} ETH deposited to the Pool!"
  },
  "odao_proposal_added": {
    "title": ":bulb: Proposal created",
    "description": "%{member_fancy} created Proposal #%{proposalID}!\n```Description:\n%{message}```"
  },
  "odao_proposal_executed": {
    "title": ":white_check_mark: Proposal Executed",
    "description": "%{member_fancy} executed Proposal #%{proposalID}!\n```Description:\n%{message}```\n```Total Votes:\n%{vote_graph}```"
  },
  "odao_proposal_voted": {
    "title": ":writing_hand: Proposal Voted",
    "description": "%{member_fancy} voted %{decision} Proposal #%{proposalID}!\n```Description:\n%{message}```\n```Total Votes:\n%{vote_graph}```"
  },
  "odao_proposal_cancelled": {
    "title": ":no_entry_sign: Cancelled Proposal",
    "description": "%{member_fancy} cancelled Proposal #%{proposalID}!\n```Description:\n%{message}```\n```Total Votes:\n%{vote_graph}```"
  },
  "odao_member_join": {
    "title": ":new: oDAO Member Joined",
    "description": "%{member_fancy} joined the oDAO with a Bond of %{rplBondAmount} RPL!"
  },
  "odao_member_leave": {
    "title": ":leaves: oDAO Member Left",
    "description": "%{nodeAddress_fancy} left the oDAO!"
  },
  "odao_member_kicked": {
    "title": ":boot: oDAO Member Kicked",
    "description": "%{nodeAddress_fancy} was kicked from the oDAO!"
  },
  "node_register_event": {
    "title": ":mailbox_with_mail: Node Registered",
    "description": "%{node_fancy} registered as a Node Operator!"
  },
  "rpl_inflation_event": {
    "title": ":chart_with_upwards_trend: RPL Inflation Occurred",
